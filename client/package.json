    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.17.1"
  },
  "scripts": {
    "start": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3000 --open",
    "build": "webpack -p --progress --colors",
    "lint": "tslint -t stylish '{src,__tests__}/**/*.{ts,tsx}'",
    "test": "jest --watchAll --no-cache",
    "docs": "npm run docs-html && opn dist/docs/index.html",
    "docs-html": "typedoc src/index.ts --excludePrivate --mode file --theme minimal --out dist/docs"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testRegex": "<rootDir>(__tests__/).*\\.(test|spec)\\.(ts|js)$"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "rx": "^4.1.0",
    "rx-dom": "^7.0.3"
  }
